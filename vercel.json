


{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/resources/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; font-src 'self';"
      },
      "dest": "/resources/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}